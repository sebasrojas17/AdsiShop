.AlignNav
  display flex
  align-items center
  flex-flow row wrap
.AlignItemsNavbar
  display flex
  justify-content center
.Header-container
  background-color #428bca
  color white
.Header-container .navbar-toggle
  color white
  font-weight bold
  font-size 25px
.Header-container .Login
  color white
  font-weight bold
  font-size 20px
  text-transform uppercase
.Header-containerAlignV
  display flex
  align-items center
.Iconsearch
  font-size 20px
.Header-containerAlignV > div > h1
  margin-top 0
  margin-bottom 0
.Header-containerAlignV > div > a
  color white
  text-decoration none
.NavbarMargin
  margin-bottom 0
.navbar-nav > li > a
  color #363636
  margin .5em .5em
a.active
  background-color #428bca !important
  color white !important
a.active:focus
  background-color inherit
.Menu-container
  background-color #f7f7f9
.MarginTopSearch
  margin-top 20px
.btn.Btn-adsi
  background-color #428bca
  color white
.btn.Btn-adsi:hover
  background-color #428bca
.MarginTopSearch
  margin-top 11px
  margin-bottom 1em
@media(max-width 990px){
  .MarginSearch-main{
    margin-bottom 1em
  }
}
